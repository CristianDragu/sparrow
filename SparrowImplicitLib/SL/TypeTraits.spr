import DefaultHashFunction;

public class[rtct] DefaultTypeTraits
{
    public fun equal(l, r: @AnyType) = l == r;
    public fun less(l, r: @AnyType) = l < r;
    public fun hash(val: @AnyType): SizeType
    {
        if[ct] ( isValid(SizeType(val hash)) )
            return val hash;
        else if[ct] ( isValid(SizeType(val.get() hash)) )
        {
            return val.get() hash;
        }
        else
            return defaultHash(val);
    }
}

