
fun[rtct] asInt(src: StringRef)       = StringCvtImpl.atoi(src.cStr());
fun[rtct] asLong(src: StringRef)      = StringCvtImpl.atoll(src.cStr());
fun[rtct] asDouble(src: StringRef)    = StringCvtImpl.atof(src.cStr());

package StringCvtImpl
{
    fun[rtct, native("atoi")] atoi(str: @Char): Int;
    fun[rtct, native("atoll")] atoll(str: @Char): Long;
    fun[rtct, native("atof")] atof(str: @Char): Double;
}
