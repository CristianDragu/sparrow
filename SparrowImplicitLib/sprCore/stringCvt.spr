import basicDecls(StringRef, Char, Int, Long, Double, typeOf)

[rtct]
    fun asInt(src: StringRef)       = StringCvtImpl.atoi(src.cStr())
    fun asLong(src: StringRef)      = StringCvtImpl.atoll(src.cStr())
    fun asDouble(src: StringRef)    = StringCvtImpl.atof(src.cStr())

package StringCvtImpl
    [rtct]
        [native("atoi")] fun atoi(str: @Char): Int
        [native("atoll")] fun atoll(str: @Char): Long
        [native("atof")] fun atof(str: @Char): Double
