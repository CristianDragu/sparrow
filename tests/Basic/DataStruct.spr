datatype MyType
    x: Int;
    y: Int;

datatype MyType2
    x: MyType;
    y: Double;

var i: Int;
var a: MyType;
var b: MyType2;

[native("test")] fun test(n: Int)
{
    assignInt(i, 1);
    assignInt(a.x, 10);
    assignInt(a.y, 11);
    assignInt(b.x.x, 20);
    assignInt(b.x.y, 21);

    writeLnInt(i);
    writeLnInt(a.x);
    writeLnInt(a.y);
    writeLnInt(b.x.x);
    writeLnInt(b.x.y);
}

