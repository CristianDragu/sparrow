//!! -t "SparrowImplicitLib.spr" -fno-main

class MyVector(t: Type)
{
    var start, end: t;

    fun print()
    {
        write(start);
        write(' ');
        write('-');
        write(' ');
        writeLn(end);
    }
}

fun[native("test")] test(n: Int)
{
    writeLn(sizeOf(Byte));
    writeLn(sizeOf(UByte));
    writeLn(sizeOf(Short));
    writeLn(sizeOf(UShort));
    writeLn(sizeOf(Int));
    writeLn(sizeOf(UInt));
    writeLn(sizeOf(Long));
    writeLn(sizeOf(ULong));
    writeLn(sizeOf(Float));
    writeLn(sizeOf(Double));

    writeLn('-');

    writeLn(sizeOf(@Byte));
    writeLn(sizeOf(@UByte));
    writeLn(sizeOf(Short@));
    writeLn(sizeOf(UShort@));
    writeLn(sizeOf(Int@));
    writeLn(sizeOf(UInt@));
    writeLn(sizeOf(Long@));
    writeLn(sizeOf(ULong@));
    writeLn(sizeOf(Float@));
    writeLn(sizeOf(Double@));

    writeLn('-');


    var v1: Byte;      writeLn(sizeOf(v1));
    var v2: UByte;     writeLn(sizeOf(v2));
    var v3: Short;     writeLn(sizeOf(v3));
    var v4: UShort;    writeLn(sizeOf(v4));
    var v5: Int;       writeLn(sizeOf(v5));
    var v6: UInt;      writeLn(sizeOf(v6));
    var v7: Long;      writeLn(sizeOf(v7));
    var v8: ULong;     writeLn(sizeOf(v8));
    var v9: Float;     writeLn(sizeOf(v9));
    var v10: Double;   writeLn(sizeOf(v10));
}


