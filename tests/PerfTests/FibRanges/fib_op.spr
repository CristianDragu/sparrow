// !! -O2

import std.algorithms;
import std.rMath;
import std.tuple;

fun fib(n: UInt): UInt {
    var p: UInt*UInt = 0 ~ 1;   // Last 2 entries
    for i = 0..n
        p = p.v2 ~ (p.v1 + p.v2);
    return p.v2;
}
fun isOdd x = x%2 != 0;


fun sprMain
{
    if ( programArgs.size() < 2 )
        return;
    var n = programArgs(1) asInt;

    var res = 1...n map \fib filter \isOdd map \sqr sum;
    cout << res << endl;
}

